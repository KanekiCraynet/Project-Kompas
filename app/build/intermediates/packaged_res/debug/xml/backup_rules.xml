<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude sensitive data from backup -->
    <exclude domain="sharedpref" path="sensitive_prefs.xml"/>
    <exclude domain="database" path="sensitive_data.db"/>
    
    <!-- Include app data -->
    <include domain="sharedpref" path="app_preferences.xml"/>
    <include domain="database" path="compass_data.db"/>
</full-backup-content>
